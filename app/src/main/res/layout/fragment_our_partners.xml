<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#CFD8DC"
  >


    <TextView
        android:id="@+id/OurPartnets"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="17dp"
        android:textSize="30dp"
        android:textColor="#000000"
        android:text="Our partners"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:id="@+id/geotheImage"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_marginTop="26dp"
        app:srcCompat="@drawable/goethe"
        android:layout_below="@+id/OurPartnets"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:id="@+id/g"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        app:srcCompat="@drawable/dt"
        android:layout_below="@+id/geotheImage"
        android:layout_alignLeft="@+id/geotheImage"
        android:layout_alignStart="@+id/geotheImage"
        android:layout_alignRight="@+id/geotheImage"
        android:layout_alignEnd="@+id/geotheImage" />

    <ImageView
        android:id="@+id/mf"
        android:layout_width="match_parent"
        android:layout_height="120dp"
        app:srcCompat="@drawable/scc"
        android:layout_marginBottom="14dp"
        android:layout_above="@+id/eImage"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:id="@+id/eImage"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        app:srcCompat="@drawable/alf_logo_2012_resize_0"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />





</RelativeLayout>
